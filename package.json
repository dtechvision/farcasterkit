{
  "name": "farcasterkit",
  "version": "0.1.0",
  "author": "fuegoblanco",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/farcasterkit.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/farcasterkit.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/farcasterkit.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@farcaster/auth-client": "^0.1.0",
    "@farcaster/auth-kit": "^0.2.1",
    "@farcaster/connect-kit": "^0.0.31",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "axios": "^1.6.5",
    "react": ">=16",
    "react-dom": "^18.2.0",
    "tsup": "^7.2.0",
    "viem": "^2.7.19"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@size-limit/preset-small-lib": "^11.0.2",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addons": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@swc/core": "^1.3.96",
    "babel-loader": "^9.1.3",
    "husky": "^9.0.11",
    "react-is": "^18.2.0",
    "size-limit": "^11.0.2",
    "ts-loader": "^9.5.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
